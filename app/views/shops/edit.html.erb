<h1>Edit This Shop</h1>

<%= render partial: "/shared/error_list", locals: {new_object: @shop} %>

<%= form_for @shop do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :image %>
<%= f.file_field :image%>
<%= f.submit %>
<% end %>
