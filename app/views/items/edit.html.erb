<h1>Edit Items</h1>

<%= render partial: "/shared/error_list", locals: {new_object: @item} %>

<%= form_for @item do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :price %>
<%= f.number_field :price %>
<%= f.label :quantity %>
<%= f.number_field :quantity %>
<%= f.label :image %>
<%= f.file_field :image%>
<%= f.submit %>
<% end %>