<h1>Login</h1>

<div><%= flash["error"] %></div>

<%= form_tag '/login' do %>
    <%= label :username, "Username" %>
    <%= text_field_tag :username %>
    <%= label :password, "Password" %>
    <%= password_field_tag :password %>
    <%= submit_tag 'Login' %>
<% end %>
<%= link_to "Create Account", new_user_path %>