<h1>Edit User</h1>

<%= render partial: "/shared/error_list", locals: {new_object: @user} %>

<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :password%>
<%= f.password_field :password %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.submit %>
<% end %>